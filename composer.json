{
  "name": "sinclairt/track",
  "description": "Track changes to your models and any custom events you'd like to keep track of",
  "license": "MIT",
  "authors": [
    {
      "name": "Tom Sinclair",
      "email": "tom.sinclair31@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "laracasts/presenter": "^0.2.1",
    "sinclairt/api-foundation": "^1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1",
    "laracasts/testdummy": "~2.0",
    "laravel/laravel": "dev-develop"
  },
  "autoload": {
    "classmap": [
      "src/config"
    ],
    "psr-0": {
      "Sinclair\\Track\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "scripts": {
    "phpunit": "./vendor/bin/phpunit;"
  },
  "config": {
    "preferred-install": "dist"
  }
}
